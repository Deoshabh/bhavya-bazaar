# Production environment settings
NODE_ENV=production
PORT=3003
HOST=bhavyabazaar.com
WDS_SOCKET_HOST=api.bhavyabazaar.com
WDS_SOCKET_PORT=3003

# Security settings
HTTPS=true
SSL_CRT_FILE=./ssl/cert.pem
SSL_KEY_FILE=./ssl/key.pem
REACT_APP_SECURE=true

# Build settings
DISABLE_ESLINT_PLUGIN=true
GENERATE_SOURCEMAP=false

# API endpoints (all HTTPS)
REACT_APP_API_URL=https://api.bhavyabazaar.com/api/v2
REACT_APP_BACKEND_URL=https://api.bhavyabazaar.com/
REACT_APP_SOCKET_URL=wss://api.bhavyabazaar.com:3003

# Feature flags
REACT_APP_ENV=production
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_EXHIBITOR_FEATURES=true
