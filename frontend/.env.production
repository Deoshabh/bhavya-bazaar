# Production environment settings
NODE_ENV=production
PORT=3003
GENERATE_SOURCEMAP=false

# Security settings
HTTPS=true
DANGEROUSLY_DISABLE_HOST_CHECK=false

# Build optimization
CI=false
DISABLE_ESLINT_PLUGIN=true
INLINE_RUNTIME_CHUNK=true
IMAGE_INLINE_SIZE_LIMIT=10000

# API endpoints
REACT_APP_API_URL=https://api.bhavyabazaar.com
REACT_APP_BACKEND_URL=https://api.bhavyabazaar.com/
REACT_APP_SOCKET_URL=wss://bhavyabazaar.com:3003

# Feature flags
REACT_APP_ENV=production
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_EXHIBITOR_FEATURES=true
